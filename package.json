{
  "name": "telegram-group-bot",
  "version": "1.0.0",
  "description": "Telegram group management bot with Cloudflare Turnstile verification",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --exec ts-node src/main.ts",
    "migration:run": "typeorm migration:run -d dist/config/database.js",
    "migration:generate": "typeorm migration:generate -d src/config/database.ts",
    "migration:create": "typeorm migration:create",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "telegram",
    "bot",
    "group-management",
    "turnstile"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.0",
    "@fastify/view": "^8.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "fastify": "^4.25.2",
    "grammy": "^1.20.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
